{
    "name": "plural-arguments",
    "version": "3.1.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "A tool to implement plural arguments in a Curry program",
    "category": [ "Logic", "Programming" ],
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "dependencies": {
        "base"          : ">= 3.0.0, < 4.0.0",
        "abstract-curry": ">= 3.0.0, < 4.0.0",
        "currypath"     : ">= 3.0.0, < 4.0.0",
        "directory"     : ">= 3.0.0, < 4.0.0",
        "filepath"      : ">= 3.0.0, < 4.0.0",
        "frontend-exec" : ">= 3.0.0, < 4.0.0",
        "process"       : ">= 3.0.0, < 4.0.0"
    },
    "executable": { "name": "curry-plural",
                    "main": "TransformPluralArguments"
                  },
    "exportedModules": [ "Plural", "TransformPluralArguments" ],
    "configModule": "ConfigPluralPackage",
    "testsuite": {
        "src-dir": "examples",
        "script" : "test.sh"
    },
    "source": {
      "git": "https://github.com/curry-packages/plural-arguments.git",
      "tag": "$version"
    }
}
